{
	"name": "easierprogramming",
	"displayName": "Easier Programming",
	"description": "The best extension to make your life easier using VSCode snippets!",
	"version": "0.0.4",
	"publisher": "reddyyz",
	"repository": "https://github.com/ReddyyZ/EasierProgramming",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Themes",
		"Snippets"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "easierprogramming.mrc-nodejs-backend",
				"title": "Create Nodejs Backend"
			},
			{
				"command": "easierprogramming.create-python-package",
				"title": "Create Python Package"
			}
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./src/snippets/javascript.json"
			},
			{
				"language": "typescriptreact",
				"path": "./src/snippets/typescript.json"
			},
			{
				"language": "python",
				"path": "./src/snippets/python.json"
			}
		],
		"themes": [
			{
				"label": "Easier Programming Theme",
				"path": "./src/theme/easierprogramming.json",
				"uiTheme": "vs-dark"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"path": "^0.12.7"
	}
}
